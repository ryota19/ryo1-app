<h1>kogotoユーザー一覧</h1>

<div class="main users-index">
    <div class="container">
        <% @users.each do |user| %>
        <div class="users-index-item">
            <div class="users-right">
                <%= link_to user.name, "/users/#{user.id}" %>
            </div>
            <div class="users-right-s">
                <%= link_to '編集', edit_user_path(user) %>
            </div>
        </div>
        <% end %>
    </div>
</div>